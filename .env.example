VITE_API_BASE_URL=
# Enable mock data ("true" or "false")
VITE_MOCK=false

# Google OAuth client ID (Google Cloud Console → APIs & Services → Credentials)
VITE_GOOGLE_CLIENT_ID=
# Google API key (Google Cloud Console → APIs & Services → Credentials)
VITE_GOOGLE_API_KEY=

# Firebase configuration
# Values from Firebase Console → Project settings → General → Your apps → Web app → SDK setup and configuration
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

# Cloudinary upload preset (Cloudinary Dashboard → Settings → Upload)
VITE_CLOUDINARY_PRESET=
# Cloudinary cloud name (Cloudinary Dashboard → Account details)
VITE_CLOUDINARY_CLOUD_NAME=
